package Bai2;

import java.util.ArrayList;

public class Students {
	private String nameStudent;
	private String nameClass;
	ArrayList<Disciplines> listMh = new ArrayList<>();
	ArrayList<Teachers> listGV = new ArrayList<>();

	public Students(String nameStudent, String nameClass, ArrayList<Disciplines> listMh, ArrayList<Teachers> listGV) {
		this.nameStudent = nameStudent;
		this.nameClass = nameClass;
		this.listMh = listMh;
		this.listGV = listGV;
	}
	public ArrayList<Disciplines> addMh(Disciplines discipline){
		listMh.add(discipline);
		return listMh;
		
	}

}
