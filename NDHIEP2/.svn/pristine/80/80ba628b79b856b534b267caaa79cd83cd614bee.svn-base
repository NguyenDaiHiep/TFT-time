package Bai3;

import java.util.ArrayList;

public class Workers extends Human implements Comparable<Workers>{
	private double weekSalary;
	private double WorkHourPerDay;
	ArrayList<Workers> listCN = new ArrayList<>();

	public Workers(String firstName, String lastname, double weekSalary, double workHourPerDay) {
		super(firstName, lastname);
		this.weekSalary = weekSalary;
		WorkHourPerDay = workHourPerDay;
	}

	public double getWeekSalary() {
		return weekSalary;
	}

	public double getWorkHourPerDay() {
		return WorkHourPerDay;
	}
	public double MonerPerHours(){
		return weekSalary/WorkHourPerDay*7;
	}
	public double compareTo(Workers works){
		if(MonerPerHours()==works.MonerPerHours())
			return 0;
		else if(MonerPerHours()>works.MonerPerHours())
			return 1;
		else
			return -1;
	}

}
