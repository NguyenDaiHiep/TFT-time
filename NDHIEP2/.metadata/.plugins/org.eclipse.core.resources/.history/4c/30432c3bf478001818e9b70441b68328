package Practice;

public class Chia5 {
	public static void chiahet5(int n){
		int count=0;
		for (int i = 0; i < n; i++) {
			switch (i%5) {
			case 1:
				count++;
				System.out.println(count);
				break;

			default:
				break;
			}
	}
	public static void main(String[] args) {
		chiahet5(100);
	}

}
